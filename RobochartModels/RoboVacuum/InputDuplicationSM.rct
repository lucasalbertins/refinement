stm InputDuplicationSM {
	var x : TripleAxis
	event input : TripleAxis
	event output1 : TripleAxis
	event output2 : TripleAxis
	initial i0
	state wait_input {
	}
	state send_output {
	}
	transition t0 {
		from i0
		to wait_input
		trigger
	}
	transition t1 {
		from wait_input
		to send_output
		trigger input ? x
	}
	transition t2 {
		from send_output
		to wait_input
		trigger
		action output1 ! x ; output2 ! x
	}
}
